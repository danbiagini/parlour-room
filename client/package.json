{
	"name": "pandemic-poker-client",
	"version": "1.0.0",
	"description": "A frontend poker game using Phaser3 built with Parceljs",
	"scripts": {
		"start": "parcel src/index.html -p 8000",
		"build": "parcel build src/index.html --out-dir dist",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"author": "danbiagini",
	"repository": {
		"type": "git",
		"url": "https://github.com/danbiagini/pandemic-poker.git"
	},
	"homepage": "https://github.com/danbiagini/pandemic-poker",
	"devDependencies": {
		"minimist": ">=1.2.2",
		"parcel-plugin-clean-easy": "^1.0.2",
		"parcel-plugin-static-files-copy": "^2.2.1",
		"typescript": "^3.8.3"
	},
	"dependencies": {
		"phaser": "^3.55.2"
	},
	"parcelCleanPaths": [
		"dist"
	],
