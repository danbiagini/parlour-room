{
  "name": "pandemic-parlour",
  "version": "1.0.0",
  "description": "Parlour games for families and friends",
  "main": "dist/src/main.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "copy-assets": "ts-node tools/copyAssets",
    "lint": "eslint src/ --ext .tsx,.ts,.js,.ejs",
    "tsc": "tsc",
    "build": "npm-run-all clean lint tsc copy-assets client-build",
    "gcp-build": "npm-run-all build sec2env",
    "dev:start": "npm-run-all build start",
    "dev": "nodemon --watch src -e ts,ejs,env --delay 5 --exec npm run dev:start",
    "test": "jest",
    "start": "node .",
    "sec2env": "ts-node tools/gcpSecrets",
    "client-build": "parcel build src/index.html --out-dir dist/public/",
    "client-build-old": "parcel build client/src/oldmaid.html --out-dir dist/public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danbiagini/pandemic-parlour.git"
  },
  "keywords": [
    "cards",
    "poker",
    "pitch",
    "old",
    "maid"
  ],
  "author": "Dan Biagini",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/danbiagini/pandemic-parlour/issues"
  },
  "homepage": "https://github.com/danbiagini/pandemic-parlour#readme",
  "dependencies": {
    "axios": "^0.19.2",
    "bootstrap": "^4.4.1",
    "dotenv": "^8.2.0",
    "ejs": "^3.0.2",
    "express": "^4.17.1",
    "googleapis": "^48.0.0",
    "jquery": "^3.5.0",
    "phaser": "^3.22.0",
    "popper.js": "^1.16.1",
    "public": "^0.1.5",
    "react": "^16.13.1",
    "react-bootstrap": "^1.0.1",
    "react-dom": "^16.13.1",
    "react-google-login": "^5.1.10",
    "react-redux": "^7.2.0",
    "react-router-bootstrap": "^0.25.0",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "winston": "^3.2.1",
    "ws": "^7.2.3"
  },
  "devDependencies": {
    "@google-cloud/secret-manager": "^3.0.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.6",
    "@types/fs-extra": "^8.1.0",
    "@types/gapi.auth2": "0.0.51",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.11.1",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-bootstrap": "^0.24.5",
    "@types/react-router-dom": "^5.1.5",
    "@types/react-test-renderer": "^16.9.2",
    "@types/redux": "^3.6.0",
    "@types/shelljs": "^0.8.7",
    "@types/ws": "^7.2.4",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "canvas": "^2.6.1",
    "eslint": "^6.8.0",
    "eslint-plugin-ejs-js": "^0.1.0",
    "eslint-plugin-react": "^7.19.0",
    "fs-extra": "^9.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.5.4",
    "jsdom": "^16.2.2",
    "nodemon": "^2.0.3",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "prettier": "2.0.5",
    "react-test-renderer": "^16.13.1",
    "rimraf": "^3.0.2",
    "sass": "^1.26.5",
    "shelljs": "^0.8.3",
    "ts-jest": "^25.5.1",
    "ts-node": "^8.8.2",
    "tslint": "^6.1.1",
    "typescript": "^3.8.3"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.1",
    "utf-8-validate": "^5.0.2"
  }
}
