{
  "name": "pandemic-parlour",
  "version": "1.0.0",
  "description": "Parlour games for families and friends",
  "main": "dist/src/server/main.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "copy-assets": "ts-node tools/copyAssets",
    "lint": "eslint src/ --ext .tsx,.ts,.js",
    "tsc": "tsc",
    "server-build": "npm run tsc",
    "build": "npm-run-all clean lint server-build client-build",
    "gcp-build": "npm-run-all build sec2env",
    "dev:server-start": "npm-run-all server-build dotenv-start",
    "dev:client": "ts-node tools/devServer",
    "dev:server": "nodemon --ignore test/ --watch src -e ts,ejs,env --delay 5 --exec npm run dev:server-start",
    "dev": "concurrently \"npm:dev:server\" \"npm:test:watch\" \"npm:dev:client\"",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "test:debug": "echo 'to debug a test, add a ''debugger'' line to the test'; node --inspect-brk node_modules/.bin/jest --runInBand",
    "dotenv-start": "node --require dotenv/config .",
    "start": "node .",
    "sec2env": "ts-node tools/gcpSecrets",
    "client-package": "parcel build src/index.html --out-dir dist/public/",
    "client-build": "npm-run-all client-test copy-assets client-package",
    "client-test": "jest src/components/test",
    "docker-build": "eval $(egrep -v '^#' .env | xargs) docker build --tag pandemicparlour-postgres tools/dockerbuild/",
    "docker-run": "docker run -d --env-file .env -p 5432:5432  pandemicparlour-postgres",
    "db-migrate": "eval $(egrep -v '^#' .env | xargs) graphile-migrate watch",
    "db-reset": "ts-node tools/dbReset",
    "db-fixtures": "ts-node tools/dbFixtures"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danbiagini/pandemic-parlour.git"
  },
  "keywords": [
    "cards",
    "poker",
    "pitch",
    "old",
    "maid"
  ],
  "author": "Dan Biagini",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/danbiagini/pandemic-parlour/issues"
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "not IE_Mob 11",
    "maintained node versions"
  ],
  "homepage": "https://github.com/danbiagini/pandemic-parlour#readme",
  "dependencies": {
    "@graphile-contrib/pg-simplify-inflector": "^6.0.0",
    "@reduxjs/toolkit": "^1.3.6",
    "@types/connect-pg-simple": "^4.2.0",
    "axios": "^0.19.2",
    "bootstrap": "^4.4.1",
    "connect-pg-simple": "^6.1.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "googleapis": "^48.0.0",
    "grommet": "^2.14.0",
    "grommet-icons": "^4.4.0",
    "jquery": "^3.5.0",
    "loglevel": "^1.6.8",
    "phaser": "^3.22.0",
    "popper.js": "^1.16.1",
    "postgraphile": "^4.7.0",
    "public": "^0.1.5",
    "react": "^16.13.1",
    "react-bootstrap": "^1.0.1",
    "react-dom": "^16.13.1",
    "react-google-login": "^5.1.20",
    "react-redux": "^7.2.0",
    "react-router-bootstrap": "^0.25.0",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "styled-components": "^5.1.1",
    "winston": "^3.2.1",
    "ws": "^7.2.3"
  },
  "devDependencies": {
    "@google-cloud/secret-manager": "^3.0.0",
    "@testing-library/dom": "^7.21.0",
    "@testing-library/jest-dom": "^5.11.1",
    "@testing-library/react": "^10.4.7",
    "@types/cookie": "^0.4.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.6",
    "@types/express-session": "^1.17.0",
    "@types/fs-extra": "^8.1.0",
    "@types/gapi.auth2": "0.0.51",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.11.1",
    "@types/parcel-bundler": "^1.12.1",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-bootstrap": "^0.24.5",
    "@types/react-router-dom": "^5.1.5",
    "@types/react-test-renderer": "^16.9.2",
    "@types/redux": "^3.6.0",
    "@types/shelljs": "^0.8.7",
    "@types/supertest": "^2.0.9",
    "@types/ws": "^7.2.4",
    "@types/yargs": "^15.0.5",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "canvas": "^2.6.1",
    "concurrently": "^5.2.0",
    "cookie": "^0.4.1",
    "cookie-parser": "^1.4.5",
    "dotenv-expand": "^5.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-ejs-js": "^0.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "fs-extra": "^9.0.0",
    "graphile-migrate": "^0.3.1",
    "http-proxy-middleware": "^1.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.5.4",
    "jsdom": "^16.2.2",
    "nodemon": "^2.0.3",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "patch-package": "^6.2.2",
    "pg": "^8.2.1",
    "prettier": "2.0.5",
    "react-test-renderer": "^16.13.1",
    "rimraf": "^3.0.2",
    "sass": "^1.26.5",
    "shelljs": "^0.8.3",
    "supertest": "^4.0.2",
    "ts-jest": "^25.5.1",
    "ts-node": "^8.8.2",
    "tslint": "^6.1.1",
    "typescript": "^3.8.3",
    "yargs": "^15.4.1"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.1",
    "utf-8-validate": "^5.0.2"
  }
}
